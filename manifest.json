{
  "manifest_version": 2,

  "name": "Reactor",
  "description": "React right from your browser",
  "version": "1.0",

  "browser_action": {
    "default_icon": {
        "19": "icon.png",
        "38": "icon-38x38.png"
    },
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab"
  ]
}
